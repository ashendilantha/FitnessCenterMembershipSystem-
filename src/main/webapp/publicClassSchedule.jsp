<%@ page contentType="text/html;charset=UTF-8" pageEncoding="UTF-8" %><%@ page import="lk.sliit.fitnesscenter.fitnesscentermembershipsystem.*, java.util.List" %><%@ page import="lk.sliit.fitnesscenter.fitnesscentermembershipsystem.dao.FitnessClassDAO" %><%@ page import="lk.sliit.fitnesscenter.fitnesscentermembershipsystem.dao.TrainerDAO" %><%@ page import="lk.sliit.fitnesscenter.fitnesscentermembershipsystem.model.FitnessClass" %><%@ page import="lk.sliit.fitnesscenter.fitnesscentermembershipsystem.model.Trainer" %><%@ page import="lk.sliit.fitnesscenter.fitnesscentermembershipsystem.model.YogaClass" %><%@ page import="lk.sliit.fitnesscenter.fitnesscentermembershipsystem.model.WeightTrainingClass" %><!DOCTYPE html><html lang="en"><head>  <meta charset="UTF-8">  <meta name="viewport" content="width=device-width, initial-scale=1.0">  <title>Class Schedule | Pulse Gym</title>  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">  <style>    body {      background: #181c23;      color: #e0e0e0;      font-family: Arial, sans-serif;    }    .schedule-header {      color: #27ef8c;      margin-top: 32px;      margin-bottom: 16px;      text-align: center;    }    .class-simple {      border: 1px solid #333;      border-radius: 8px;      padding: 18px 16px;      margin-bottom: 18px;      background: #22242a;    }    .class-simple h5 {      margin-bottom: 8px;      color: #27ef8c;      font-size: 1.15rem;    }    .class-simple p {      margin-bottom: 4px;      font-size: 0.98rem;    }    .footer {      background: #181c23;      color: #27ef8c;      padding: 32px 0 0 0;      margin-top: 40px;      border-top: 2px solid #1e7a53;      text-align: center;      font-size: 0.98rem;    }    a {      color: #27ef8c;      text-decoration: none;    }    a:hover {      color: #1e7a53;      text-decoration: underline;    }  </style></head><body><nav class="navbar navbar-expand-lg navbar-dark" style="background:#15171b;">  <div class="container">    <a class="navbar-brand" href="index.jsp" style="color:#27ef8c;">Pulse Gym</a>    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav">      <span class="navbar-toggler-icon"></span>    </button>    <div class="collapse navbar-collapse" id="navbarNav">      <ul class="navbar-nav ms-auto">        <li class="nav-item"><a class="nav-link" href="index.jsp">Home</a></li>        <li class="nav-item"><a class="nav-link" href="addMember.jsp">Register</a></li>        <li class="nav-item"><a class="nav-link" href="viewPlans.jsp">Plans</a></li>        <li class="nav-item"><a class="nav-link" href="publicTrainers.jsp">Our Trainers</a></li>        <li class="nav-item"><a class="nav-link active" href="publicClassSchedule.jsp">Class Schedule</a></li>        <li class="nav-item"><a class="nav-link" href="loginMember.jsp">Member Login</a></li>      </ul>    </div>  </div></nav><div class="container" style="max-width: 700px;">  <h2 class="schedule-header">Class Schedule</h2>  <p class="text-center mb-4">Browse our fitness classes and find the perfect session for your training needs.</p>  <%    FitnessClassDAO classDAO = new FitnessClassDAO();    TrainerDAO trainerDAO = new TrainerDAO();    List<FitnessClass> classes = classDAO.getAllClasses();    if (classes.isEmpty()) {  %>  <div class="alert alert-warning text-center">No classes found</div>  <%  } else {    List<Trainer> trainers = trainerDAO.getAllTrainers();    for (FitnessClass fc : classes) {      Trainer trainer = null;      for (Trainer t : trainers) {        if (t.getTrainerId().equals(fc.getTrainerId())) {          trainer = t;          break;        }      }  %>  <div class="class-simple">    <h5><%= fc.getClassName() %></h5>    <p><b>Date & Time:</b> <%= fc.getFormattedDateTime() %> </p>    <% if (trainer != null) { %>    <p><b>Trainer:</b> <%= trainer.getName() %></p>    <% } %>    <p>      <b>Type:</b>      <% if (fc instanceof YogaClass) { %>      Yoga Class      <% String groupType = ((YogaClass) fc).getGroupType(); if (groupType != null && !groupType.isEmpty()) { %>      <% } %>      <% } else if (fc instanceof WeightTrainingClass) { %>      Weight Training      <% String focus = ((WeightTrainingClass) fc).getFocusArea(); if (focus != null && !focus.isEmpty()) { %>      <% } %>      <% }  %>    </p>  </div>  <%      }    }  %></div><div class="footer mt-5">  &copy; 2025 Pulse Gym. ALL RIGHTS RESERVED.</div><script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script></body></html>